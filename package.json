{
  "name": "coinmonitor",
  "version": "1.0.0",
  "description": "An application which real-time monitoring crypto-currency (e.g. bitcoin) ",
  "main": "app/main.js",
  "scripts": {
    "start": "node_modules\\electron-prebuilt\\dist\\electron .",
    "devpro": "set NODE_ENV=\"development\" node_modules\\electron-prebuilt\\dist\\electron .",
    "dev": "node node_modules\\webpack-dev-server\\bin\\webpack-dev-server.js --hot",
    "deploy": "node node_modules\\webpack\\bin\\webpack.js -p --progress --colors --config webpack.production.config.js",
    "pack": "node node_modules\\electron-packager\\cli.js ./app coinMonitor --out=dist/win --platform=win32 --arch=x64 --icon=icon.ico --asar=true --asar-unpack-dir=\"config\" --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myzenon/coinMonitor.git"
  },
  "keywords": [
    "bitcoin",
    "monitor"
  ],
  "author": "Zenon 'SI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/myzenon/coinMonitor/issues"
  },
  "homepage": "https://github.com/myzenon/coinMonitor#readme",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.2.5",
    "flexboxgrid": "^6.3.0",
    "material-ui": "^0.15.1",
    "node-sass": "^3.8.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-flexbox-grid": "^0.9.6",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.5.0",
    "react-tap-event-plugin": "^1.0.0",
    "reset-css": "^2.0.2011012603",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
